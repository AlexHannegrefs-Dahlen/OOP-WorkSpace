package store.grocery;

public class GroceryItem implements GroceryInterface {
	double weight;
	double price;

	@Override
	public double getWeight() {
		return this.weight;
	}

	@Override
	public double getPrice() {
		return this.price;
	}

	@Override
	public void setWeight(double weight) {
		this.weight = weight;
	}

	@Override
	public void setPrice(double price) {
		this.price = price;
	}

}
