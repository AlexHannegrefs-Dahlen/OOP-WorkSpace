package tic.tac.toe.model.board;

public class Board {
	private BoardSpace[] Boardspaces = new BoardSpace[9];
	
	Board(){
		for(int i = 0; i < this.Boardspaces.length; i++){
			Boardspaces[i] = new BoardSpace((char) (i),false);
		}
	}

	/**
	 * @return the boardspaces
	 */
	public BoardSpace[] getBoardspaces() {
		return Boardspaces;
	}

	/**
	 * @param boardspaces the boardspaces to set
	 */
	public void setBoardspaces(BoardSpace[] boardspaces) {
		Boardspaces = boardspaces;
	}
	
}
