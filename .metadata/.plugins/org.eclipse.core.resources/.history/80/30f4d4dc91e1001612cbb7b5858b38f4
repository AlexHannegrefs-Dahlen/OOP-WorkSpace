package tic.tac.toe.model.board;

public class BoardSpace {

	private char symbol;

	BoardSpace(char symbol) {
		this.setSymbol(symbol);
	}

	/**
	 * @return the symbol
	 */
	public char getSymbol() {
		return symbol;
	}

	/**
	 * @param symbol
	 *            the symbol to set
	 */
	public void setSymbol(char symbol) {
		this.symbol = symbol;
	}
}
